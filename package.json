{
  "name": "breach-protocol-autosolver",
  "version": "1.3.0",
  "description": "Automatically solve breach protocol minigame in Cyberpunk 2077",
  "main": "src/index.ts",
  "private": true,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    }
  },
  "scripts": {
    "test": "jest",
    "watch-ts": "tsc -w && tsc-alias -w",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently \"npm:watch-ts\" \"npm:watch-node\"",
    "watch-test": "npm run test -- --watchAll",
    "release": "standard-version -s",
    "build": "tsc && tsc-alias && ts-node scripts/build/index.ts",
    "postinstall": "patch-package"
  },
  "author": "Marcin Cichocki <marcincichocki1993@gmail.com>",
  "homepage": "https://github.com/marcincichocki/breach-protocol-autosolver#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcincichocki/breach-protocol-autosolver.git"
  },
  "bugs": "https://github.com/marcincichocki/breach-protocol-autosolver/issues",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.8",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/sharp": "^0.27.1",
    "concurrently": "^5.3.0",
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "nexe": "^4.0.0-beta.18",
    "nodemon": "^2.0.7",
    "patch-package": "^6.4.7",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.2.9",
    "typescript": "^4.1.3",
    "upx": "^1.0.6",
    "zip-a-folder": "^0.1.0"
  },
  "dependencies": {
    "@octokit/rest": "^18.4.0",
    "commander": "^7.2.0",
    "inquirer": "^8.0.0",
    "iohook": "^0.6.6",
    "is-wsl": "^2.2.0",
    "open": "^8.0.4",
    "ora": "^5.4.0",
    "sanitize-filename": "^1.6.3",
    "screenshot-desktop": "^1.12.3",
    "sharp": "^0.27.2",
    "sound-play": "^1.1.0",
    "tesseract.js": "^2.1.4"
  }
}
