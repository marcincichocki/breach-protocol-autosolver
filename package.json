{
  "name": "breach-protocol-autosolver",
  "version": "2.8.5",
  "description": "Automatically solve breach protocol minigame in Cyberpunk 2077",
  "main": "dist/main.js",
  "private": true,
  "sideEffects": [
    "src/electron/renderer/styles/fonts.css",
    "src/electron/worker/error-handler.ts"
  ],
  "jest": {
    "reporters": [
      "default",
      "github-actions"
    ],
    "preset": "ts-jest",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance improvements"
      }
    ]
  },
  "build": {
    "appId": "com.marcincichocki.breach-protocol-autosolver",
    "productName": "Breach Protocol Autosolver",
    "files": [
      "dist/**/*"
    ],
    "extraFiles": [
      "resources/tessdata/*",
      {
        "from": "dist",
        "to": "resources",
        "filter": "*.json"
      },
      {
        "from": "src/electron/renderer/assets/fonts/Rajdhani/OFL.txt",
        "to": "resources"
      }
    ],
    "directories": {
      "buildResources": "resources",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "extraFiles": [
        "resources/win32/**/*"
      ],
      "asarUnpack": [
        "node_modules/screenshot-desktop/lib/win32"
      ]
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "asarUnpack": [
        "node_modules/sharp"
      ]
    },
    "extraMetadata": {
      "main": "dist/main.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "release": "standard-version -s",
    "postinstall": "patch-package",
    "webpack:watch": "webpack --config configs/webpack.config.ts --watch",
    "webpack:build": "webpack --config configs/webpack.config.ts --mode production",
    "electron:run": "electron dist/main.js",
    "electron:build": "npm run webpack:build && electron-builder build"
  },
  "author": "Marcin Cichocki <marcincichocki1993@gmail.com>",
  "homepage": "https://github.com/marcincichocki/breach-protocol-autosolver#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcincichocki/breach-protocol-autosolver.git"
  },
  "bugs": "https://github.com/marcincichocki/breach-protocol-autosolver/issues",
  "license": "MIT",
  "devDependencies": {
    "@types/dompurify": "^2.3.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.5",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/sharp": "^0.31.0",
    "@types/styled-components": "^5.1.26",
    "@types/tar": "^6.1.3",
    "@types/uuid": "^8.3.4",
    "css-loader": "^6.7.2",
    "electron": "^20.3.5",
    "electron-builder": "^23.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "patch-package": "^6.5.0",
    "source-map-loader": "^4.0.1",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-license-plugin": "^4.2.2"
  },
  "dependencies": {
    "@react-icons/all-files": "^4.1.0",
    "date-fns": "^2.29.3",
    "dompurify": "^2.4.0",
    "electron-first-run": "^3.0.0",
    "electron-is-accelerator": "^0.2.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "fs-extra": "^10.1.0",
    "is-wsl": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "sanitize-filename": "^1.6.3",
    "screenshot-desktop": "^1.12.7",
    "sharp": "^0.31.1",
    "styled-components": "^5.3.6",
    "tar": "^6.1.12",
    "tesseract.js": "^2.1.5",
    "uuid": "^9.0.0"
  }
}
