{
  "name": "breach-protocol-autosolver",
  "version": "2.0.0-rc.0",
  "description": "Automatically solve breach protocol minigame in Cyberpunk 2077",
  "main": "dist/main.js",
  "private": true,
  "sideEffects": false,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    }
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "perf",
        "section": "Performance improvements"
      }
    ]
  },
  "build": {
    "appId": "com.marcincichocki.breach-protocol-autosolver",
    "productName": "Breach Protocol Autosolver",
    "files": [
      "dist/**/*"
    ],
    "extraFiles": [
      "resources/BreachProtocol.traineddata",
      {
        "from": "dist",
        "to": "resources",
        "filter": "*.txt"
      }
    ],
    "directories": {
      "buildResources": "resources",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "extraFiles": [
        "resources/win32/**/*"
      ],
      "asarUnpack": [
        "node_modules/screenshot-desktop/lib/win32"
      ]
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "asarUnpack": [
        "node_modules/sharp"
      ]
    },
    "extraMetadata": {
      "main": "dist/main.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "release": "standard-version -s",
    "postinstall": "patch-package",
    "webpack:watch": "webpack --config configs/webpack.config.ts --watch",
    "webpack:build": "webpack --config configs/webpack.config.ts --mode production",
    "electron:run": "electron dist/main.js",
    "electron:build": "npm run webpack:build && electron-builder build"
  },
  "author": "Marcin Cichocki <marcincichocki1993@gmail.com>",
  "homepage": "https://github.com/marcincichocki/breach-protocol-autosolver#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcincichocki/breach-protocol-autosolver.git"
  },
  "bugs": "https://github.com/marcincichocki/breach-protocol-autosolver/issues",
  "license": "MIT",
  "devDependencies": {
    "@types/dompurify": "^2.2.3",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/sharp": "^0.27.1",
    "@types/styled-components": "^5.1.9",
    "@types/tar": "^4.0.4",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^5.28.0",
    "css-loader": "^5.2.4",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "license-webpack-plugin": "^2.3.19",
    "patch-package": "^6.4.7",
    "standard-version": "^9.3.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.1.0",
    "typescript": "^4.3.2",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "date-fns": "^2.22.1",
    "dompurify": "^2.3.1",
    "electron-first-run": "^3.0.0",
    "electron-is-accelerator": "^0.2.0",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "is-electron-renderer": "^2.0.1",
    "is-wsl": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "sanitize-filename": "^1.6.3",
    "screenshot-desktop": "^1.12.5",
    "sharp": "^0.29.0",
    "styled-components": "^5.2.3",
    "tar": "^6.1.0",
    "tesseract.js": "^2.1.4",
    "uuid": "^8.3.2"
  }
}
